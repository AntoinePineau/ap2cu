<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/Screen">    

<TextView android:id="@+id/foodLabel" style="@style/Title" android:text="@string/food.label"></TextView>

<TableRow android:id="@+id/pRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/pLabel" style="@style/Label" android:text="@string/food_p.label"></TextView>
  <EditText android:id="@+id/pField" style="@style/Field" android:text=""></EditText>
  <TextView android:id="@+id/gramsLabel1" style="@style/Label" android:text="@string/grams.label"></TextView>
</TableRow>
<TableRow android:id="@+id/lRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/lLabel" style="@style/Label" android:text="@string/food_l.label"></TextView>
  <EditText android:id="@+id/lField" style="@style/Field" android:text=""></EditText>
  <TextView android:id="@+id/gramsLabel2" style="@style/Label" android:text="@string/grams.label"></TextView>
</TableRow>
<TableRow android:id="@+id/fRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/fLabel" style="@style/Label" android:text="@string/food_f.label"></TextView>
  <EditText android:id="@+id/fField" style="@style/Field" android:text=""></EditText>
  <TextView android:id="@+id/gramsLabel3" style="@style/Label" android:text="@string/grams.label"></TextView>
</TableRow>
<TableRow android:id="@+id/gRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/gLabel" style="@style/Label" android:text="@string/food_g.label"></TextView>
  <EditText android:id="@+id/gField" style="@style/Field" android:text=""></EditText>
  <TextView android:id="@+id/gramsLabel4" style="@style/Label" android:text="@string/grams.label"></TextView>
</TableRow>
<TableRow android:id="@+id/bRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/bLabel" style="@style/Label" android:text="@string/food_b.label"></TextView>
  <EditText android:id="@+id/bField" style="@style/Field" android:text="100"></EditText>
  <TextView android:id="@+id/gramsLabel5" style="@style/Label" android:text="@string/grams.label"></TextView>
</TableRow>
<TableRow android:id="@+id/qRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/qLabel" style="@style/Label" android:text="@string/food_q.label"></TextView>
  <EditText android:id="@+id/qField" style="@style/Field" android:text="100"></EditText>
  <TextView android:id="@+id/gramsLabel6" style="@style/Label" android:text="@string/grams.label"></TextView>
</TableRow>
<TableRow android:id="@+id/intRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/intLabel" android:text=""></TextView>
  <CheckBox android:id="@+id/intField" style="@style/Check" android:text="@string/i.label" android:checked="true"></CheckBox>
</TableRow>
<TableRow android:id="@+id/resultRow" android:layout_width="fill_parent" android:layout_height="wrap_content">
  <TextView android:id="@+id/result" style="@style/Result" android:text="0"></TextView>
  <TextView android:id="@+id/pointsLabel" style="@style/Result" android:text="@string/points.label"></TextView>
</TableRow>

</TableLayout>
